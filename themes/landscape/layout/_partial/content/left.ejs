<div class="about-background row no-margin" style="background-image: url(<%= content.image %>);">
    <div class="col-md-6 about-black-box">
        <h3 class="section-heading">
            <%= content.title %>
        </h3>
        <%- content.content %>
        
        <% if(content.ctaLink) { %>
            <a href="<%= content.ctaLink %>" class="btn btn-default">
                <%= content.ctaText %>
            </a>
        <% } %>

    </div>
    <div class="col-md-6"></div>
</div>